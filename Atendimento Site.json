              "id": "5265bd8e-43d0-4cf3-9f45-3de1863a0ca5",
              "name": "messages",
              "value": "={{\n  (() => {\n    // Inicializa oldMessages (tratando casos em que $json.messages n√£o existe)\n    let oldMessages = $json.messages || [];\n    try {\n      if (typeof oldMessages === \"string\") oldMessages = JSON.parse(oldMessages);\n      if (!Array.isArray(oldMessages)) oldMessages = [];\n    } catch (e) { oldMessages = []; }\n \n    // Verifica se OpenAI1 foi executado e tem conte√∫do\n    let openAIMessage = null;\n    try {\n      openAIMessage = $('OpenAI').item?.json?.text;\n    } catch (e) {} // Ignora se o n√≥ n√£o foi executado\n\n    // Verifica se OpenAI1 foi executado e tem conte√∫do\n    let openAIMessage1 = null;\n    try {\n      openAIMessage1 = $('OpenAI1')?.item?.json?.content;\n    } catch (e) {} // Ignora se o n√≥ n√£o foi executado\n\n    // Verifica se Webhook foi executado e tem conte√∫do\n    let webhookMessage = null;\n    try {\n      webhookMessage = $('Webhook1')?.item?.json?.body.content\n    } catch (e) {} // Ignora se o n√≥ n√£o foi executado\n\n // Verifica se Webhook foi executado e tem conte√∫do\n    let pdf = null;\n    try {\n      pdf = $('PDF')?.item?.json?.pdf;\n    } catch (e) {} // Ignora se o n√≥ n√£o foi executado\n\n    // Adiciona mensagens v√°lidas ao array\n    if (openAIMessage) oldMessages.push(openAIMessage);\n    if (openAIMessage1) oldMessages.push(openAIMessage1);\n    if (webhookMessage) oldMessages.push(webhookMessage);\n    if (pdf) oldMessages.push(pdf);\n\n    return oldMessages;\n  })()\n}}",
              "type": "string"
